Title: 
 ELSA linear growth model for IALSA tri-study coordinated analysis
 
Data:
  File is ELSArn_Disease Groups BL at W1_long to wide_141024.dat ;

Variable:
  Names are 
  PID dob time yrseduc sex blage htnonly dmonly htndm
  neither blANG blCHF blMI blSTK cogflag eversmoke
  blHTN blDM irecall animal prospect drecall;

USEVAR are   sex eversmoke
             blCHF blMI blSTK
             blHTN blDM time
             blHTNDM
             ed age age_sex
             age_htn age_dm 
             sex_htn sex_dm 
             y;

Missing are all (999); 
                   
!USEOBSERVATIONS are cogflg1A EQ 0;

CLUSTER = PID; 

USEOBSERVATIONS = (cogflag EQ 0);

WITHIN = time;

BETWEEN = age sex ed age_sex eversmoke blCHF blMI blSTK
          blHTN blDM blHTNDM age_htn age_dm
          sex_htn sex_dm;

  Define:  
           age=blage-80;
           ed=yrseduc-11;
           age_sex=sex*age;
           blHTNDM=blHTN*blDM;
           age_htn=age*blHTN;
           age_dm=age*blDM;
          ! agehtndm=age*htn_dmw1;
           sex_htn=sex*blHTN;
           sex_dm=sex*blDM;
          ! sexhtndm=sexrA*htn_dmw1;
           y=irecall;
           
  ANALYSIS:  TYPE = TWOLEVEL RANDOM; ESTIMATOR=MLR;

  MODEL:  

  ! Level-1, time-level model
      %WITHIN%                                     ! Residual included by default
           linear | y ON time;                  ! Fixed linear time slope

  ! Level-2, person-level model
      %BETWEEN%
           y*;                                  ! Random intercept included by default
           linear*;                                ! Random linear time slope
           y WITH linear*;                      ! Intercept-linear covariance
           y linear ON sex age ed eversmoke
           blCHF blMI blSTK blHTN blDM blHTNDM
           age_sex age_htn age_dm
           sex_htn sex_dm;


  OUTPUT: sampstat; STDYX;



